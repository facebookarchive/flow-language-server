{
  "name": "flow-language-server",
  "version": "0.0.3",
  "main": "server/lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf server/lib",
    "dev": "yarn watch",
    "build": "yarn run clean && babel -s --copy-files --ignore node_modules,spec ./server/src -d ./server/lib",
    "build-atom": "babel -s --copy-files --ignore node_modules,spec ./atom-languageserver-flow/src -d ./atom-languageserver-flow/lib",
    "prepare": "yarn build",
    "test": "jest",
    "watch": "yarn build -- --watch & yarn build-atom -- --watch"
  },
  "bin": {
    "flow-language-server": "./server/lib/bin/cli.js"
  },
  "devDependencies": {
    "atom-ide-ui": "^0.1.4",
    "babel-cli": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-idx": "^1.5.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-fbjs-opensource": "^1.0.0",
    "fs-extra": "^3.0.1",
    "jest": "^19.0.2",
    "vscode-languageserver-types": "^3.2.0"
  },
  "dependencies": {
    "async-to-generator": "^1.1.0",
    "event-kit": "^2.0.0",
    "fs-plus": "^2.8.2",
    "fuzzaldrin-plus": "^0.4.1",
    "idx": "^1.5.0",
    "log4js": "^1.1.1",
    "lru-cache": "^4.0.1",
    "mkdirp": "^0.5.1",
    "nuclide-commons": "^0.1.4",
    "nullthrows": "^1.0.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0",
    "semver": "^5.3.0",
    "shell-quote": "^1.6.0",
    "simple-text-buffer": "^9.2.11",
    "temp": "^0.8.3",
    "through": "^2.3.6",
    "vscode-languageserver": "^3.2.0",
    "vscode-uri": "^1.0.0"
  },
  "jest": {
    "testMatch": [
      "**/*-spec.js",
      "**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "flow-vscode/*",
      "server/lib/*",
      "atom-languageserver-flow/lib/"
    ]
  },
  "files": [
    "server/lib"
  ]
}
